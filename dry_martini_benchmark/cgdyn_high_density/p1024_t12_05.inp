[ENERGY]
forcefield          = GroMartini
electrostatic       = RF
cg_switchdist_vdw   = 10.0
cg_cutoffdist_vdw   = 11.0
cg_cutoffdist_ele   = 11.0
cg_cellsize_min     = 17.0
cg_pairlistdist_vdw = 14.0
cg_pairlistdist_ele = 14.0
dielec_const        = 15.0
output_style        = GENESIS
upper_force_value   = 10.0
