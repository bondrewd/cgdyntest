tpcontrol           = LANGEVIN
temperature         = 303.15

[BOUNDARY]
type                = PBC
box_size_x          = 1597.4049
box_size_y          = 1598.3130
box_size_z          = 1599.5110
domain_x            = 16
domain_y            =  8
domain_z            =  8
